{
  "name": "balance-crypto",
  "version": "0.0.17",
  "type": "module",
  "description": "Get wallet balance for 300+ cryptos with a unified interface",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./": {
      "import": "./dist/index.js"
    },
    "./package.json": {
      "import": "./package.json"
    }
